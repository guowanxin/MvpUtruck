<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical">

    <tdh.ifm.android.imatch.app.ui.view.MyTitleView
        android:id="@+id/titleview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20px"
                android:background="@drawable/bg_white">

                <LinearLayout
                    android:id="@+id/llayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/ll_member_level"
                        android:layout_width="match_parent"
                        android:layout_height="80px"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="21px"
                            android:text="@string/txt_star"
                            android:textColor="@color/text_prompt" />

                        <TextView
                            style="@style/fs_row_text_left"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_member" />

                        <View
                            android:layout_width="0px"
                            android:layout_height="1px"
                            android:layout_weight="1" />

                        <TextView
                            android:id="@+id/tv_member_level"
                            style="@style/fs_row_text_right"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_primay"
                            android:textColor="@color/tab_text_press" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="31px"
                            android:layout_marginRight="64px"
                            android:src="@mipmap/icon_arrow" />

                    </LinearLayout>

                    <View style="@style/line_style" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="80px"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="21px"
                            android:text="@string/txt_star"
                            android:textColor="@color/text_prompt" />

                        <TextView
                            style="@style/fs_row_text_left"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_name" />

                        <EditText
                            android:id="@+id/et_name"
                            style="@style/fs_row_text_right"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="101px"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="@string/prompt_name"
                            android:textColor="@color/text_prompt"
                            android:textColorHint="@color/text_prompt_right" />
                    </LinearLayout>

                    <View style="@style/line_style" />

                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20px"
                android:background="@drawable/bg_white">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="80px"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="21px"
                            android:text="@string/txt_star"
                            android:textColor="@color/text_prompt" />

                        <TextView
                            style="@style/fs_row_text_left"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_plate_number" />

                        <EditText
                            android:id="@+id/et_plate_number"
                            style="@style/fs_row_text_right"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="73px"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="@string/prompt_plate_number"
                            android:textColor="@color/text_prompt"
                            android:textColorHint="@color/text_prompt_right" />
                    </LinearLayout>

                    <View style="@style/line_style" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="80px"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="21px"
                            android:text="@string/txt_star"
                            android:textColor="@color/text_prompt" />

                        <TextView
                            style="@style/fs_row_text_left"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_car_length" />

                        <EditText
                            android:id="@+id/et_car_length"
                            style="@style/fs_row_text_right"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="51px"
                            android:layout_weight="1"
                            android:background="@null"
                            android:focusable="false"
                            android:hint="@string/prompt_car_length"
                            android:textColor="@color/text_prompt"
                            android:textColorHint="@color/text_prompt" />


                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="64px"
                            android:gravity="center"
                            android:src="@mipmap/icon_arrow" />
                    </LinearLayout>


                    <View style="@style/line_style" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="80px"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="21px"
                            android:text="@string/txt_star"
                            android:textColor="@color/text_prompt" />

                        <TextView
                            style="@style/fs_row_text_left"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_load" />

                        <EditText
                            android:id="@+id/et_load"
                            style="@style/fs_row_text_right"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="55px"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="@string/prompt_load"
                            android:inputType="number"
                            android:textColor="@color/text_prompt"
                            android:textColorHint="@color/text_prompt_right" />
                    </LinearLayout>


                    <View style="@style/line_style" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="80px"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="21px"
                            android:text="@string/txt_star"
                            android:textColor="@color/text_prompt" />

                        <TextView
                            style="@style/fs_row_text_left"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_car_type" />

                        <EditText
                            android:id="@+id/et_car_type"
                            style="@style/fs_row_text_right"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="99px"
                            android:layout_weight="1"
                            android:background="@null"
                            android:focusable="false"
                            android:hint="@string/prompt_car_type"
                            android:textColor="@color/text_prompt"
                            android:textColorHint="@color/text_prompt" />


                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="64px"
                            android:gravity="center"
                            android:src="@mipmap/icon_arrow" />
                    </LinearLayout>

                </LinearLayout>
            </RelativeLayout>


            <View
                android:id="@+id/view_first"
                android:layout_width="match_parent"
                android:layout_height="10px"
                android:layout_marginTop="20px" />

            <Button
                android:id="@+id/btn_submit_audit"
                android:layout_width="match_parent"
                android:layout_height="96px"
                android:layout_marginLeft="100px"
                android:layout_marginRight="100px"
                android:layout_marginTop="37px"
                android:background="@drawable/btn_bg"
                android:gravity="center"
                android:text="@string/btn_member_submit_audit"
                android:textColor="@color/white"
                android:textSize="36px" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>